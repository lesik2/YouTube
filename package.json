{
    "name": "youtube",
    "version": "1.0.0",
    "description": "Application for searching films and watching trailers",
    "main": "index.js",
    "scripts": {
        "start": "webpack serve --open --config ./webpack.config.js --env mode=dev",
        "build": "webpack --config ./webpack.config.js --env mode=prod",
        "lint": "eslint --ext .ts,.tsx",
        "lint:fix": "npm run lint -- --fix",
        "prepare": "husky install",
        "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
        "test": "jest",
        "test:cypress": "start-server-and-test start http://localhost:9000 cypress",
        "cypress": "cypress open"
    },
    "resolutions": {
        "styled-components": "^5"
    },
    "keywords": [
        "youtube",
        "films",
        "trailers",
        "categories"
    ],
    "author": "Alexey Pukhalskiy",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.23.0",
        "@babel/preset-env": "^7.22.20",
        "@babel/preset-react": "^7.22.15",
        "@babel/preset-typescript": "^7.23.2",
        "@cypress/webpack-preprocessor": "^6.0.0",
        "@testing-library/dom": "^9.3.3",
        "@testing-library/jest-dom": "^6.1.4",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.5.1",
        "@types/jest": "^29.5.5",
        "@types/node": "^20.8.6",
        "@types/react": "^18.2.25",
        "@types/react-dom": "^18.2.11",
        "@types/webpack-env": "^1.18.2",
        "@typescript-eslint/eslint-plugin": "^6.7.4",
        "@typescript-eslint/parser": "^6.7.4",
        "babel-loader": "^9.1.3",
        "clean-webpack-plugin": "^4.0.0",
        "css-loader": "^6.8.1",
        "cypress": "^13.3.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-cypress": "^2.15.1",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-webpack-plugin": "^4.0.1",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.5.3",
        "husky": "^8.0.3",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-fetch-mock": "^3.0.3",
        "lint-staged": "^14.0.1",
        "prettier": "^3.0.3",
        "react-scripts": "^5.0.1",
        "react-test-renderer": "^18.2.0",
        "start-server-and-test": "^2.0.1",
        "style-loader": "^3.3.3",
        "ts-loader": "^9.5.0",
        "typescript": "^5.2.2",
        "webpack": "^5.88.2",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^1.9.7",
        "@types/react-redux": "^7.1.27",
        "axios": "^1.5.1",
        "eslint": "^8.51.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^8.1.3",
        "styled-components": "^6.0.8",
        "ts-jest": "^29.1.1"
    }
}
